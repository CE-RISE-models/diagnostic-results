                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "component_test_results": {
                    "description": "Test results for components",
                    "items": {
                        "$ref": "#/$defs/ComponentTestResults"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "condition_metrics": {
                    "description": "Health and condition measurements",
                    "items": {
                        "$ref": "#/$defs/ConditionMetrics"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "data_sanitization": {
                    "description": "Data erasure and sanitization records",
                    "items": {
                        "$ref": "#/$defs/DataSanitization"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "diagnostic_event": {
                    "description": "Foundation diagnostic event with context",
                    "items": {
                        "$ref": "#/$defs/DiagnosticEvent"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "predictive_analytics": {
                    "description": "Predictive maintenance outputs",
                    "items": {
                        "$ref": "#/$defs/PredictiveAnalytics"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "quality_assessment": {
                    "description": "Quality grading and assessment",
                    "items": {
                        "$ref": "#/$defs/QualityAssessment"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                },
                "service_actions": {
                    "description": "Service and repair actions performed",
                    "items": {
                        "$ref": "#/$defs/ServiceActions"
                    },
                    "type": [
                        "array",
                        "null"
                    ]
                }
            },
            "title": "DiagnosticResults",
            "type": "object"
        },
        "DiagnosticSoftware": {
            "additionalProperties": false,
            "description": "Information about diagnostic software or tool used",
            "properties": {
                "tool_certification": {
